{
  "manifest_version": 3,
  "name": "Site Blocker",
  "version": "1.1",
  "description": "Dynamically blocks websites with overlay and timer",
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
